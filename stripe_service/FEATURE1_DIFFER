import difflib

def print_diff(string1, string2):
    """
    Prints the difference between two strings with marked differences using '**'.
    """
    # Split the strings into individual lines
    lines1 = string1.splitlines()
    lines2 = string2.splitlines()

    # Create a Differ object
    differ = difflib.Differ()

    # Compare the lines
    diff = list(differ.compare(lines1, lines2))

    # Print the result with marked differences
    for line in diff:
        prefix = line[:2]
        if prefix == "? ":
            continue
        elif prefix == "+ ":
            print("**" + line[2:] + "**")
        elif prefix == "- ":
            print("**" + line[2:] + "**")
        else:
            print(line[2:])